{
  "name": "markdown-docs-skill",
  "version": "1.0.0",
  "category": "documentation",
  "description": "Markdown文档生成工具。Use when documenting APIs, generating documentation, or when user mentions 'OpenAPI', 'Swagger', 'API docs'.",
  "author": "godlike-kimi-skills",
  "license": "MIT",
  "entry": "main.py",
  "keywords": ["markdown", "documentation", "readme", "changelog", "docs"],
  "triggers": ["markdown", "readme", "documentation", "changelog", "api doc", "generate docs"],
  "dependencies": ["jinja2"],
  "min_kimi_version": "1.0.0",
  "config": {
    "default_template": "default",
    "include_toc": true,
    "preserve_existing": true
  }
}
