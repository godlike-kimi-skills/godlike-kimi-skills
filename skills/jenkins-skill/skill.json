{
  "name": "jenkins-skill",
  "category": "cicd",
  "version": "1.0.0",
  "description": "Jenkins流水线管理 - 支持Job创建、Build触发、Pipeline配置和节点管理",
  "author": "Godlike-Kimi-Skills",
  "license": "MIT",
  "entry_point": "main.py",
  "config": {
    "url": {
      "type": "string",
      "required": true,
      "description": "Jenkins server URL"
    },
    "username": {
      "type": "string",
      "required": true,
      "description": "Jenkins username"
    },
    "token": {
      "type": "string",
      "required": true,
      "description": "Jenkins API token"
    },
    "timeout": {
      "type": "integer",
      "required": false,
      "default": 30,
      "description": "Request timeout in seconds"
    }
  },
  "capabilities": [
    "job_create",
    "job_update",
    "job_delete",
    "build_trigger",
    "build_monitor",
    "pipeline_config",
    "node_manage",
    "credential_manage"
  ],
  "tags": ["jenkins", "cicd", "devops", "pipeline", "automation"],
  "dependencies": ["python-jenkins>=1.8.0", "requests>=2.28.0"],
  "python_version": ">=3.8"
}
